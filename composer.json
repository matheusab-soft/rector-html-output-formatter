{
    "version": "1.1",
    "name": "matheusab/rector-html-output-formatter",
    "description": "HTML Output Formatter for the Rector PHP Library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Alves Barbosa",
            "email": "contato@matheusab.com",
            "homepage": "https://www.matheusab.com",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MAB\\Rector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MAB\\RectorHtmlFormatterTest\\": "tests/test_src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests --colors"
    }
}
